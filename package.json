{
  "name": "freefolk-micro-template",
  "private": true,
  "version": "1.5.0",
  "description": "Freefolk microservice template",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker": "docker build -t freefolk-micro-template ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seirius/freefolk-micro-template.git"
  },
  "author": "seirius",
  "bugs": {
    "url": "https://github.com/seirius/freefolk-micro-template/issues"
  },
  "homepage": "https://github.com/seirius/freefolk-micro-template#readme",
  "dependencies": {
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^4.6.0",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "env-var": "^5.2.0",
    "puppeteer": "^5.2.1",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-adblocker": "^2.11.6",
    "puppeteer-extra-plugin-stealth": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^6.11.8",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.21",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.28",
    "@types/puppeteer": "^3.0.1",
    "@types/supertest": "^2.0.8",
    "jest": "^26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
